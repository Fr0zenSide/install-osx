[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	lg = log --oneline --graph
	ld = diff HEAD..HEAD^
        ci-undo = reset --soft HEAD~1
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	ui = true
[core]
	excludesfile = .gitignore
	editor = emacs
