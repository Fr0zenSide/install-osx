[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	lg = log --oneline --graph
	ld = diff HEAD^..HEAD
        ci-undo = reset --soft HEAD~1
	rm-last-ci = reset --hard HEAD^
	stats = diff --stat
        sss = diff --shortstat
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	ui = true
[core]
	excludesfile = .gitignore
	editor = emacs
[user]
